{
  "name": "pinvues-hub",
  "description": "Simple apps made with VueJS (Just for practicing VueJS). I started this as part of the #100DaysOfCode challenge.",
  "version": "1.0.0",
  "author": "PinguinoSod <pinguino.sod@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "test": "jest"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@vue/test-utils": "^1.0.0-beta.19",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.8.0",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "jest": "^24.8.0",
    "json-loader": "^0.5.7",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "vue": "^2.5.11",
    "vue-i18n": "^8.0.0",
    "vue-jest": "^3.0.4",
    "vue-loader": "^14.2.2",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.6.10",
    "vue-test-utils": "^1.0.0-beta.11",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.5.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "vue-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  }
}
